{
  "name": "budget-calculator",
  "version": "0.0.1-alpha",
  "description": "Open source budget calculator",
  "author": "fathalfath30 <fathalfath30@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "npm run development",
    "development": "mix",
    "watch": "mix watch",
    "watch-poll": "mix watch -- --watch-options-poll=1000",
    "hot": "mix watch --hot",
    "prod": "npm run production",
    "production": "mix --production"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/custom-cz-conventional-changelog-for-jira",
      "jiraOptional": true
    }
  },
  "engines": {
    "npm": "please-use-yarn",
    "yarn": ">= 1.19.1"
  },
  "devDependencies": {
    "axios": "^0.21",
    "laravel-mix": "^6.0.6",
    "lodash": "^4.17.19",
    "postcss": "^8.1.14",
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "commitizen": "^4.2.3",
    "custom-cz-conventional-changelog-for-jira": "^0.0.0-semantically-released",
    "pre-commit": "^1.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "nodemon": "2.0.7"
  }
}
